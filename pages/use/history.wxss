 /*列表*/
 .lpbox{ border-bottom: solid 1px #eee; padding: 40rpx 0;}
 .lpbox image{ width: 245rpx; height: 183rpx; border-radius: 10rpx; margin:0 20rpx 0 30rpx;}
 .lpmsg view{ margin-bottom: 5rpx;}
 .lpmsg view:last-child{margin-bottom: 0;}
 .lpmsg .lpname{font-size: 34rpx; display: flex; align-items: center;}
 .lpmsg .lpname image{ width: 63rpx; height: 31rpx; position: absolute; top:0; left: 0; margin:0}
 .lpmsg .lp_txt{ font-size: 24rpx; color:#333 ;}
 .lpmsg .lp_money{ font-size: 24rpx; color: #FF3831; margin: 0;}
 .lpmsg .lp_money i{font-size: 36rpx;}
 .lpmsg .lp_tag i{ font-size: 22rpx; color:#999; border:solid 1px #CECECE; padding: 5rpx 8rpx; display: inline-block; margin-right: 10rpx;}
 .lpmsg .house_type{ width: 63rpx; height: 31rpx;margin:0 10rpx 0 0; position: relative; font-size: 24rpx; text-align: center; line-height: 31rpx; color: #fff; background: url(data:image/png;base64,) no-repeat 0 0; background-size:100%; overflow: hidden;}

 .flex-wrp{ display: flex;}
 .times{font-size: 26rpx; margin-left: 30rpx; margin-bottom: 20rpx;margin-bottom: 20rpx;}